{
  "manifest_version": 3,
  "name": "Home Depot Article Capturer",
  "version": "1.0",
  "description": "Capture the slug and URL of Home Depot articles.",
"permissions": [
  "scripting",
  "tabs",
  "activeTab"
],
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  }
}
